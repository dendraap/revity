{
  "expo": {
    "name": "Revity",
    "slug": "revity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_revity.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash_revity.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dendra.revity"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon_revity.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dendra.revity"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon_revity.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b1414675-39b4-405f-a40f-ffbfe9c10dac"
      }
    },
    "owner": "dendra"
  }
}
